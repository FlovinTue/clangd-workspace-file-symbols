{
  "name": "clangd-workspace-file-symbols",
  "displayName": "Clangd Workspace File Symbols",
  "description": "Upgrade your `Go To Symbols In Workspace` to fake the ReSharper/Jetbrains `Search Everywhere` feature!",
  "version": "1.0.0",
  "publisher": "flovintue",
  "license": "MIT",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "clang",
    "clangd",
    "c++"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:cuda-cpp",
    "onLanguage:objective-c",
    "onLanguage:objective-cpp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FlovinTue/clangd-workspace-file-symbols.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "clangdWorkspaceFileSymbols",
      "properties": {
        "clangdWorkspaceFileSymbols.inclusionFilter": {
          "type": "string",
          "default": "",
          "description": "Filter workspace file request by inclusion pattern. Directly forwarded to findFiles. See findFiles at https://code.visualstudio.com/api/references/vscode-api#workspace"
        },
        "clangdWorkspaceFileSymbols.exclusionFilter": {
          "type": "string",
          "default": "",
          "description": "Filter workspace file request by exclusion pattern. Directly forwarded to findFiles. See findFiles at https://code.visualstudio.com/api/references/vscode-api#workspace"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "@clangd/vscode-clangd": "^0.0.0",
    "vscode-languageclient": "8.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  }
}